{
  "name": "carry-on",
  "private": true,
  "description": "A Store for React",
  "author": "Russ Panula <russ@dfa.net>",
  "license": "MIT",
  "devDependencies": {
    "lerna": "^3.5.1",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "dev": "run-p watch test:watch",
    "watch": "lerna exec --concurrency 5 --no-sort -- \"yarn watch | cat\"",
    "test:watch": "lerna exec --concurrency 5 --no-sort -- \"yarn test:watch | cat\"",
    "test:cov": "lerna exec --concurrency 5 --no-sort -- \"yarn test:cov | cat\"",
    "build": "lerna exec --concurrency 5 --no-sort -- \"yarn build | cat\""
  }
}
