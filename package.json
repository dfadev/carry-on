{
  "name": "carry-on",
  "private": true,
  "description": "A Store for React",
  "author": "Russ Panula <russ@dfa.net>",
  "license": "MIT",
  "devDependencies": {
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build-store": "cd packages/carry-on-store && npm run build",
    "build-utils": "cd packages/carry-on-utils && npm run build",
    "build-react": "cd packages/carry-on-react && npm run build",
    "build-react-forms": "cd packages/carry-on-react-forms && npm run build",
    "build-react-router": "cd packages/carry-on-react-router && npm run build",
    "build": "run-s bootstrap build-utils build-store build-react build-react-forms build-react-router",
    "watch-store": "cd packages/carry-on-store && npm run watch",
    "watch-utils": "cd packages/carry-on-utils && npm run watch",
    "watch-react": "cd packages/carry-on-react && npm run watch",
    "watch-react-forms": "cd packages/carry-on-react-forms && npm run watch",
    "watch-react-router": "cd packages/carry-on-react-router && npm run watch",
    "watch": "run-p watch-utils watch-store watch-react watch-react-forms watch-react-router",
    "test-store": "cd packages/carry-on-store && npm run test",
    "test-utils": "cd packages/carry-on-utils && npm run test",
    "test-react": "cd packages/carry-on-react && npm run test",
    "test-react-forms": "cd packages/carry-on-react-forms && npm run test",
    "test-react-router": "cd packages/carry-on-react-router && npm run test",
    "test": "run-s bootstrap test-utils test-store test-react test-react-forms test-react-router",
    "test:watch-store": "cd packages/carry-on-store && npm run test:watch",
    "test:watch-utils": "cd packages/carry-on-utils && npm run test:watch",
    "test:watch-react": "cd packages/carry-on-react && npm run test:watch",
    "test:watch-react-forms": "cd packages/carry-on-react-forms && npm run test:watch",
    "test:watch-react-router": "cd packages/carry-on-react-router && npm run test:watch",
    "test:watch": "run-p test:watch-utils test:watch-store test:watch-react test:watch-react-forms test:watch-react-router",
    "dev": "run-p watch test:watch",
    "test:cov": "lerna exec --concurrency 6 --no-sort -- \"npm run test:cov | cat\"",
    "clean": "lerna exec --concurrency 6 --no-sort -- \"npm run clean | cat\""
  },
  "dependencies": {}
}
