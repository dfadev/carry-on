{
  "name": "carry-on-utils",
  "version": "0.0.50",
  "description": "common utils for carry-on",
  "source": "./src/index.js",
  "module": "./dist/carry-on-utils.esm.js",
  "main": "./dist/carry-on-utils.cjs.js",
  "browser": "./dist/carry-on-utils.umd.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test",
    "pretest": "npm run build",
    "test": "cross-env BABEL_ENV=test jest --config ./.jest.config.js",
    "test:watch": "npm test -- --watchAll",
    "test:cov": "npm test -- --coverage",
    "build": "rollup -c rollup.config.js",
    "build-dev": "rollup -c rollup.config.js --configDebug",
    "watch": "npm run build -- -w"
  },
  "author": "Russ Panula <russ@dfa.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-eslint": "^5.0.0",
    "snapshot-diff": "^0.4.2"
  },
  "sideEffects": false,
  "dependencies": {
    "debounce-promise": "^3.1.0",
    "react-fast-compare": "^2.0.4",
    "throttle-debounce": "^2.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfadev/carry-on.git"
  }
}
