{
  "name": "carry-on-store",
  "version": "1.0.7",
  "description": "State manager",
  "source": "./src/index.js",
  "module": "./dist/carry-on-store.esm.js",
  "main": "./dist/carry-on-store.cjs.js",
  "browser": "./dist/carry-on-store.umd.js",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test",
    "pretest": "npm run build",
    "test": "cross-env BABEL_ENV=test jest --config ./.jest.config.js",
    "test:watch": "npm test -- --watchAll",
    "test:cov": "npm test -- --coverage",
    "build": "rollup -c rollup.config.js",
    "build-dev": "rollup -c rollup.config.js --configDebug",
    "watch": "npm run build -- -w"
  },
  "author": "Russ Panula <russ@dfa.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@testing-library/jest-dom": "^5.12.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "snapshot-diff": "^0.8.1"
  },
  "sideEffects": false,
  "gitHead": "e21942a1d6e23d964f9b74bd1c9c3744cb7b3c18",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "carry-on-utils": "^1.0.7",
    "immer": "^9.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dfadev/carry-on.git"
  }
}
