{
  "presets": [
    ["env", {
      "modules": false,
      "targets": { "browsers": ["last 2 versions"] }
    }],
    "react",
    "stage-2",
    "optimized"
  ],
  "env": {
    "development": {
      "plugins": ["lodash", "external-helpers"],
    },
    "production": {
      "plugins": ["lodash", "external-helpers"],
    },
    "test": {
      "presets": ["env", "react", "stage-2"]
    }
  }
}
